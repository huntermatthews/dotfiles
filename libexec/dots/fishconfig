#! /usr/bin/env fish  

set help "
Usage: dots builder [options] suffix target_file
Options:
  -d, --debug        Enable debug mode
  -h, --help         Show this help
"

source $DOTS/libexec/dots/lib-common.fish
or begin
	echo "FATAL: Unable to source lib-common.fish library" 
	exit 10
end

dots builder .fish $DOTS_L/config%fish%config.fish.symlink
